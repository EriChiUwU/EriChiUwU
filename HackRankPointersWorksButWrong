#include <stdio.h>
#include <iostream>
using namespace std;


void update(int &a, int &b)
{
    int sum = a + b;
    int sub = a - b;
    a = sum;
    b = sub;

    if (b < 0)
    {
        b = -b;
    }
    else
    {
        b;
    }

}

int main() {
    int a, b;

    cin >> a;
    cin >> b;

    update(a, b);

    cout << a << endl;
    cout << b;
    return 0;
}
